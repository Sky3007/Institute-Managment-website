<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Registerservlet</servlet-name>
        <servlet-class>com.tech.servlet.Registerservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addAccServlet</servlet-name>
        <servlet-class>com.tech.servlet.addAccServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewstudentservlet</servlet-name>
        <servlet-class>com.tech.servlet.viewstudentservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewaccountServlet</servlet-name>
        <servlet-class>com.tech.servlet.viewaccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>studentlogout</servlet-name>
        <servlet-class>com.tech.servlet.studentlogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutadmin</servlet-name>
        <servlet-class>com.tech.servlet.logoutadmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewallstudent</servlet-name>
        <servlet-class>com.tech.servlet.viewallstudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletestudent</servlet-name>
        <servlet-class>com.tech.servlet.deletestudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updatestudent</servlet-name>
        <servlet-class>com.tech.servlet.updatestudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editstudentform</servlet-name>
        <servlet-class>com.tech.servlet.editstudentform</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registerservlet</servlet-name>
        <url-pattern>/Registerservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addAccServlet</servlet-name>
        <url-pattern>/addAccServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewstudentservlet</servlet-name>
        <url-pattern>/viewstudentservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewaccountServlet</servlet-name>
        <url-pattern>/viewaccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>studentlogout</servlet-name>
        <url-pattern>/studentlogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutadmin</servlet-name>
        <url-pattern>/logoutadmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewallstudent</servlet-name>
        <url-pattern>/viewallstudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletestudent</servlet-name>
        <url-pattern>/deletestudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updatestudent</servlet-name>
        <url-pattern>/updatestudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editstudentform</servlet-name>
        <url-pattern>/editstudentform</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
